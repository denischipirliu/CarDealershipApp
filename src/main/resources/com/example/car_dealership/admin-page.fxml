<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="500.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.car_dealership.AdminPageController">
    <stylesheets>
        <String fx:value="styles.css" />
    </stylesheets>
   <children>
      <Button fx:id="addCar" mnemonicParsing="false" onAction="#handleAddCarClick" text="Add Car" />
      <Button fx:id="viewCars" mnemonicParsing="false" onAction="#handleViewCarsClick" text="View Cars" />
   </children>
</VBox>
