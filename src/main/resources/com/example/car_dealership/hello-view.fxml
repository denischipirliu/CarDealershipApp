<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.car_dealership.LoginController">

    <stylesheets>
        <String fx:value="styles.css" />
    </stylesheets>
    <children>
        <TextField fx:id="usernameField" alignment="CENTER" maxWidth="200" promptText="Username">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></TextField>
        <PasswordField fx:id="passwordField" alignment="CENTER" maxWidth="200" promptText="Password">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></PasswordField>
        <Button fx:id="loginButton" text="Login">
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin></Button>
        <Button fx:id="signUpButton" text="Sign Up" />
    </children>
</VBox>
